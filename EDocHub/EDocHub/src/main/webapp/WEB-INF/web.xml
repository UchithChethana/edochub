<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
 
  <servlet>
      <servlet-name>login</servlet-name>
      <servlet-class>com.patient.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
       <servlet-name>login</servlet-name>
       <url-pattern>/log</url-pattern>  
  </servlet-mapping>
  
  <servlet>
       <servlet-name>PatientInsert</servlet-name>
       <servlet-class>com.patient.PatientInsert</servlet-class>
  </servlet>
  <servlet-mapping>
       <servlet-name>PatientInsert</servlet-name>
       <url-pattern>/insert</url-pattern>
  </servlet-mapping>
  
  <servlet>
        <servlet-name>UpdateUser</servlet-name>
        <servlet-class>com.patient.UpdateUserServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
         <servlet-name>UpdateUser</servlet-name>
         <url-pattern>/update</url-pattern>
  </servlet-mapping>
  
  <servlet>
        <servlet-name>DeletePatient</servlet-name>
        <servlet-class>com.patient.DeletePatientServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
         <servlet-name>DeletePatient</servlet-name>
         <url-pattern>/delete</url-pattern>
  </servlet-mapping>
  
  
</web-app>